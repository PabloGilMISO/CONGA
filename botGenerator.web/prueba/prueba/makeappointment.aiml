    <template>
      <think><set name="petName"><star index="1"/>ignorado<star index="2"/></set></think>
    </template>
  </category>
  <category>
    <pattern>SAVEPETNAME * *</pattern>
    <template>
      <think>
        <set name="petName">
          <srai>FUNCION</srai>
          <srai>FUNCION2 <star index="1"/></srai>
        </set>
      </think>
    </template>
  </category>
</aiml>
